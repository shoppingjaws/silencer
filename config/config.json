{
  "$schema": "https://raw.githubusercontent.com/shoppingjaws/silencer/main/config/config.schema.json",
  "rules": [
    {
      "repository": "*/*",
      "reason": "*",
      "state": "closed",
      "type": "*",
      "action": "done"
    },
    {
      "repository": "*/*",
      "reason": "*",
      "state": "open",
      "type": "*",
      "action": "done"
    }
  ],
  "log": {
    "console": "INFO",
    "file": "DEBUG"
  }
}
